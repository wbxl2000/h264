{
  "name": "h264-viewer",
  "version": "1.0.0",
  "description": "H.264解码查看器",
  "main": "server.js",
  "bin": "server.js",
  "dependencies": {
    "express": "^4.21.2"
  },
  "scripts": {
    "start": "node server.js",
    "build": "pkg ."
  },
  "pkg": {
    "assets": [
      "index.html",
      "styles.css",
      "js/**/*",
      "sample_720p30.h264"
    ],
    "targets": [
      "node18-macos-x64",
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  }
}
